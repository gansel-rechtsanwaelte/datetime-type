{
    "name": "gansel-rechtsanwaelte/datetime-type",
    "description": "Doctrine Datetime Type",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": { "Gansel\\DateTime\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Gansel\\DateTime\\Test\\": "tests/" }
    },
    "require": {
        "php": ">=7.2",
        "doctrine/dbal": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.0",
        "symfony/framework-bundle": "^3.4.26",
        "symfony/serializer": "^3.3"
    },
    "suggest": {
        "doctrine/dbal": "Database mapping types are provided",
        "symfony/framework-bundle": "An integration bundle is provided",
        "symfony/serializer": "Normalizers is provided"
    },
    "config": {
        "sort-packages": true
    }
}
